/* region Header */

header
{
    box-sizing: border-box;
    background-color: ¤BackgroundColor¤;
    box-shadow: 0 8px 24px 0 rgba(0, 0, 0, 0.151);
    max-width: 95%;
    width: 1360px;
    padding: 0.5em 1.5em;
    position: fixed;
    margin-inline: auto;
    margin-top: 2rem;
    border-radius: 8px;
    left: 0;
    right: 0;
    z-index:3;
}

#toggle-nav {
    display: none;
}

#small-pagpage-name {
    display: none;
}

#large-pagpage-name {
    display: block;
}

nav > div {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

nav > div > #toggle-nav {
    background-color: transparent;
    height: 34px;
    width: 34px;
    padding: 0;
    margin: 0;
    color: inherit;
}

nav > div > #toggle-nav > svg {
    background-color: transparent;
    height: 100%;
    width: 100%;
    color: inherit;
}

nav > div > p {
    padding: 0;
    margin: 0;
    height: fit-content; 
}

nav > ul 
{
    list-style-type: none;
    margin: 0;
    padding: 0;
	display:flex;
}

nav > ul > li
{
    float: left;
    background-clip: content-box;
	white-space:nowrap;
	transition: opacity 0.25s ease-out;
    font-weight: 600;
	opacity:1;
}


header:hover > nav > ul > li
{
	opacity:1;
    color: inherit;
}

nav > ul > li:first-child
{
    border-left: 0;
}

nav > ul > li a
{
    display: block;
    text-align: center;
    padding: 0.9em 1em;
    text-decoration: none;
    color: inherit;
}

nav > ul > li a:hover
{ 
    color: ¤ButtonColor¤;
}

nav > ul > li p
{
    padding: 0;
    margin: 0;
}

nav > ul > li > ul
{
    display:none;
}

nav > ul > li > ul > li
{
    background-clip: content-box;
	white-space:nowrap;
	transition: opacity 0.25s ease-out;

}

nav > ul > li > ul > li p{
    padding: 0.5em 1em;
}

nav > ul > li > ul > li > p
{
    padding: 0;
    margin: 0;
}


