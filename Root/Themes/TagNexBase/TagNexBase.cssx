@media screen and (min-width: 901px) {
    #native-header nav ul
    {
        align-items: center;
    }


    #native-header nav > ul > li:hover > ul 
    {
        display:block;
        list-style-type: none;
        margin: 0;
        padding: 0;
    }

    #native-header li > ul > li > a
    {
        background-color: ¤BackgroundColor¤;
    }
    #native-header li > ul > li:hover > a
    {
        background-color: ¤Blend(BackgroundColor, "black", 0.1)¤;
    }
}


@media screen and (max-width: 900px) {

    main > section
    {
        border: 0;
        border-radius: 0;
    }

    #small-pagpage-name {
        display: block;
    }

    #large-pagpage-name {
        display: none;
    }

    #toggle-nav {
        display: inline-block;
    }

    nav {
        padding: 0;
    }

    nav > ul {
        flex-direction: column;
    }

    header#native-header
    {
        margin: 0;
        max-width: 100%;
        border-radius: 0;
    }

    header ul {
        display: none;
    }

    header[data-visible] ul {
       display: flex;
    }
}